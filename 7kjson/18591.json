    {
      "text": "\t\tPossible errors",
      "resource-id": "com.andrewtretiakov.followers_assistant:id/i_h0",
      "ancestors": [
        "android.widget.TextView",
        "android.view.View",
        "java.lang.Object"
      ],
      "clickable": true,
      "class": "com.tretiakov.absframework.views.text.AbsTextView",
